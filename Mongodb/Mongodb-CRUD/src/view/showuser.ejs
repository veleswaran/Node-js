<!DOCTYPE html>
<html lang="en">
<head>
  <title>User in this page</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>

<div class="container mt-3">
  <h2>Users list table</h2>
       
  <table class="table table-dark">
    <thead>
      <tr>
        <th>name</th>
        <th>phone</th>
        <th>Age</th>
        <th>Email</th>
        <th>password</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
        <% users.map((val) => { %>
            <tr>
              <td><%= val.name %></td>
              <td><%= val.phone %></td>
              <td><%= val.age %></td>
              <td><%= val.email %></td>
              <td><%= val.pswd %></td>
              <td>
                <button> <a href="/deleteuser/<%= val._id %>">delete</a></button>
                <button> <a href="/update/<%= val._id %>">update</a></button>
              </td>
            </tr>
         <% }); %>
    </tbody>
  </table>
</div>

</body>
</html>
